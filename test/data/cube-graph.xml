<!-- This graph resembles a 3D cube.  -->

<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

  <graph id="cube" edgedefault="undirected">

    <node id="v1" />
    <node id="v2" />
    <node id="v3" />
    <node id="v4" />
    <node id="v5" />
    <node id="v6" />
    <node id="v7" />
    <node id="v8" />

    <edge id="e01" source="v1" target="v2" />
    <edge id="e02" source="v2" target="v3" />
    <edge id="e03" source="v3" target="v4" />
    <edge id="e04" source="v4" target="v1" />

    <edge id="e05" source="v5" target="v6" />
    <edge id="e06" source="v6" target="v7" />
    <edge id="e07" source="v7" target="v8" />
    <edge id="e08" source="v8" target="v5" />

    <edge id="e09" source="v1" target="v5" />
    <edge id="e10" source="v2" target="v6" />
    <edge id="e11" source="v3" target="v7" />
    <edge id="e12" source="v4" target="v8" />

  </graph>

</graphml>
